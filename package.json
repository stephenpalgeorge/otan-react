{
  "name": "otan-react",
  "version": "2.2.0",
  "description": "A tool for scaffolding react components and component libraries",
  "main": "index.js",
  "module": "index.js",
  "bin": {
    "otan-react": "./index.js"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "README.md",
    "index.js"
  ],
  "preferGlobal": true,
  "dependencies": {
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "figlet": "^1.2.4",
    "inquirer": "^7.0.0"
  },
  "devDependencies": {},
  "scripts": {
    "setup": "node lib/setup-library/exec.js",
    "new": "node lib/generate-component/exec.js",
    "delete": "node lib/delete-component/exec.js",
    "rebuild": "node lib/rebuild-library/exec.js",
    "rename": "node lib/rename-component/exec.js",
    "info": "node index"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stephenpalgeorge/otan-react.git"
  },
  "author": "stephen pal-george",
